//Problem 4
//Store the name, school, grade, section, rollno and the marks scored by the student in 3 subjects
//Print the report card of the student (You can make it look nice by using some keyboard symbols )
let name=":--Name Abhishek";
let school=":--School B.I.V.9.d";
let grade1=":--Grade chem A";
let grade2=":--Grade phy A";
let grade3=":--Grade maths A";
let section=":--Section B";
let rollno=":--Roll No 9";
let chem=":--Sub Chem Marks 70";
let phy=":--Sub Phy Marks 80";
let maths=":--Sub Maths Marks 90";
let reportcard=":--Report card"
console.log(name);
console.log(school);
console.log(grade1);
console.log(grade2);
console.log(grade3);
console.log(section);
console.log(rollno);
console.log(chem);
console.log(phy);
console.log(maths);
console.log(reportcard);